<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:glassMorphism="clr-namespace:SukiUI.Controls.GlassMorphism"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="SukiUI.Controls.GlassMorphism.SukiBlurBackground">
    <Panel>
        <glassMorphism:BlurBackground   />
        <Panel  Background="White" IsVisible="{DynamicResource IsDark}" Opacity="0.07"></Panel>
        <Panel  Background="White" IsVisible="{DynamicResource IsLight}" Opacity="0.4"></Panel>
    </Panel>

</UserControl>
